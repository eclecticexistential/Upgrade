<script>
$(document).ready(function() {
  $("button").addClass("animated bounce");
  $(".well").addClass("animated shaked");
  $("#target3").addClass("animated fadeOut");
  $("#target3").css("color", "red");
  $(".target:nth-child(2)").addClass("animated bounce");
  $("body").addClass("animated hinge");
})
<!-- nth-child will give the 2nd child in each element bounce -->
<!-- even if body not defined in html, jQuery will auto apply -->
</script>
<div class="container-fluid">
<button class="btn btn-default target" id="target3">jQuery</button>
<p class="well">does this ready doc work?</p>
</div>
<!-- This does not work when opening a web page -->